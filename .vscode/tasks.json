{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "git publish changes",
			"type": "shell",
			"command": "git",
			"args": [
				"--no-pager",
				"status"
			],
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Iniciar servidor local HTTP",
			"type": "shell",
			"command": "python",
			"args": [
				"-m",
				"http.server",
				"8000"
			],
			"isBackground": true,
			"group": "build"
		}
	]
}